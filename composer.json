{
    "name": "taronyuu/nova-mollie-tool",
    "description": "Simple tool for Laravel Nova to show your transactions from Mollie",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "mollie/mollie-api-php": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Taronyuu\\NovaMollieTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taronyuu\\NovaMollieTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
